    <!-- Custom styles for this template -->
    <style type="text/css">
      body {
        padding-top: 50px;
        overflow-y: scroll;
      }
      .starter-template {
        padding: 40px 15px;
        text-align: center;
      }
      .cover {
        background-repeat: none;
        background-size: 100%;
